from Standard.Base import all

import project.Value_Type.Value_Type

## PRIVATE
valid_parse_targets =
    ['..Boolean', '..Integer', '..Float', '..Date', '..Date_Time', '..Time']

## PRIVATE
   Checks if the type is a valid argument for `parse`.
is_a_valid_parse_target target_type:Value_Type -> Boolean =
    case target_type of
        Value_Type.Boolean -> True
        Value_Type.Integer _ -> True
        Value_Type.Float _ -> True
        Value_Type.Date -> True
        Value_Type.Date_Time _ -> True
        Value_Type.Time -> True
        _ -> False
